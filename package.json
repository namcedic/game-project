{
  "name": "game-pr",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && ts-node ./src/app.ts",
    "dev": "nodemon --exec ts-node ./src/app.ts",
    "lint": "eslint . --ext .ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:generate ./src/database/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:run",
    "db:seed": "ts-node -r tsconfig-paths/register --project ./tsconfig.json ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d src/database/datasource.ts --name src/database/seeds/db.seeder.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "express": "^4.19.2",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.2",
    "pg": "^8.11.5",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.13",
    "globals": "^15.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
